public class BattleTest
{
	public static void main(String[] args)
	{
		String fn = "centaurField.dat";
		Battlefield grid = new Battlefield(fn);
		do {
			
			System.out.println(grid);
			System.out.println();
		} while(grid.strike());
		
	}
}
